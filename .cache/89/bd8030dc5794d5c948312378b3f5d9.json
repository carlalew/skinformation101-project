ation.assign('/');\r\n        }, 3000);\r\n    }\r\n} catch (err) {\r\n    alert(err.response.data.message);\r\n    }\r\n};\r\n\r\nexport const deleteProduct = async () => {\r\n    try {\r\n      const res = await axios({\r\n        method: 'DELETE',\r\n        url: 'http://127.0.0.1:3000/api/v1/products/:id'\r\n      });\r\n      if (res.data.status = 'success') {\r\n        showAlert('success','Product Deleted!');\r\n        window.setTimeout(() => {\r\n            location.assign('/')\r\n        }, 1000);\r\n    };\r\n  } catch (err) {\r\n    console.log(err.response);\r\n    showAlert('error', 'Error deleting product! Try again.');\r\n  }\r\n};"},"lineCount":null}},"error":null,"hash":"fcceb761a27fbbcb53afa1d1b49ac625","cacheData":{"env":{}}}